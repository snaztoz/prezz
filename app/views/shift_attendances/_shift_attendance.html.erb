<div id="<%= dom_id shift_attendance %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Tenant:</strong>
    <%= shift_attendance.tenant_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= shift_attendance.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Shift occurence:</strong>
    <%= shift_attendance.shift_occurence_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Clock in at:</strong>
    <%= shift_attendance.clock_in_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Clock out at:</strong>
    <%= shift_attendance.clock_out_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Location:</strong>
    <%= shift_attendance.location %>
  </div>
</div>
